LocalPlayerTab:AddButton({
	Name = "Rejoin (Made by IY)",
	Callback = function()
      		game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
  	end    
})

LocalPlayerTab:AddButton({
	Name = "ServerHop (Made by IY)",
	Callback = function()
      		local x = {}
	for _, v in ipairs(game:GetService("HttpService"):JSONDecode(game:HttpGetAsync("https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100")).data) do
		if type(v) == "table" and v.maxPlayers > v.playing and v.id ~= game.JobId then
			x[#x + 1] = v.id
		end
	end
	if #x > 0 then
		game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, x[math.random(1, #x)])
		end
  	end    
})


local SoundTab = Window:MakeTab({
    Name = "Audio Player",
    Icon = "rbxassetid://9555767321",
    PremiumOnly = false
})

SoundTab:AddTextbox({
    Name = "Imput sound",
    Default = "",
    TextDisappear = true,
    Callback = function(sounf)
        print(sounf)
        Songs = sounf
    end      
})

SoundTab:AddButton({
    Name = "Play Song",
    Callback = function()
          print("sounf played")
        sounfer.Name = "Sound"
        sounfer.SoundId = "rbxassetid://"..Songs
        sounfer.Parent = game.Workspace
        
        sounfer:Play()
      end    
})
SoundTab:AddSlider({
    Name = "Volume Slider",
    Min = 0.1,
    Max = 10,
    Default = 1,
    Color = Color3.fromRGB(255,70,70),
    Increment = 0.1,
    ValueName = "",
    Callback = function(Value)
    local Volume = Value
        sounfer.Volume = Value
    end    
})

SoundTab:AddToggle({
	Name = "Loop song?",
	Default = false,
	Callback = function(Value)
		loop = Value
	end    
})

task.defer(function ()
    while true do
        task.wait(0.1)
        
        
        if loop then
	sounfer.Looped = loop
	end
   end
end)

