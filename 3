local pm = 0
BuyTab:AddButton({
	Name = "Buy all Speed Upgrades",
	Callback = function()
      		while pm <= 73 do
      		   game:GetService("ReplicatedStorage").Events.GameEvents.UpgradeSpeed:FireServer() 
      		   pm = pm + 1
      		end
  	end    
})
