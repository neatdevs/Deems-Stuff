	if bool then
		loop = true
		utils.RenderStepped:Connect(function()
			if loop then
				task.wait()
				chat("ff others ff others ff others ff others ff others ff others ")
			else
				loop = false
			end
		end)
	elseif not bool then
		loop = false
	end
end)

