function chat(msg)
	utils.Players:Chat(msg)
end

function attachbp()
	chat("stun me")
	wait(1)
	chat("punish me")
	wait(1)
	chat("sit me")
	wait(1)
	chat("unpunish me")
end

function fixbp()
	chat("respawn me")
	chat("jail me")
	chat("skydive me me")
	task.wait(5)
	attachbp()
	chat("unskydive me")
	chat("reset me")
end

function breakbp()
	attachbp()
	wait(1)
	chat("skydive me me")
end

function fixcam()
	utils.Camera:remove()
	wait(.1)
	repeat wait() until utils.Character ~= nil 
	utils.Camera.CameraSubject = utils.Character:FindFirstChildWhichIsA("Humanoid")
	utils.Camera.CameraType = "Custom"
	utils.Player.CameraMinZoomDistance = 0.5
	utils.Player.CameraMaxZoomDistance = 400
	utils.Player.CameraMode = "Classic"
	utils.Player.Character.Head.Anchored = false
end

