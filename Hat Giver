if game.PlaceId == 7154305304 then
	repeat task.wait() until game:IsLoaded()

    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

    OrionLib:MakeNotification({
        Name = "Revived!!! 19/04/23",
        Content = "Added Syntax and Sukma with RCD workaround",
        Image = "rbxassetid://4483345998",
        Time = 5
    })

wait(5)

OrionLib:MakeNotification({
	Name = "Looking for more hubs/scripts",
	Content = "DM me on discord if you want me to add any hubs that isn't in here, this also goes for the commonly used tab. (neatdev#7275)",
	Image = "rbxassetid://4483345998",
	Time = 8
})

local Window = OrionLib:MakeWindow({Name = "Hat Giver RCD", HidePremium = true, SaveConfig = false, ConfigFolder = "OrionTest"})

makefolder("Hat Giver")

local Common = Window:MakeTab({
	Name = "Commonly Used",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Customise = Window:MakeTab({
	Name = "Customise Hats",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

