--// Customise Hats \\--

Customise:AddTextbox({
	Name = "Custom Hat Giver",
	Default = "",
	TextDisappear = true,
	Callback = function(ID)
		Mats = ID
        game.Players:Chat("-gh "..Mats)
	end	  
})

Customise:AddButton({
	Name = "Save hats?",
	Callback = function()
        writefile("Hat Giver/Saved Hats.txt", Mats)
  	end    
})

Customise:AddButton({
	Name = "Load save hats?",
	Callback = function()
        game.Players:Chat("-gh "..readfile("Hat Giver/Saved Hats.txt"))
  	end    
})

